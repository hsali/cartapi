{
    "name": "hsali/shoppingcart_api",
    "description": "Laravel Shoppingcart",
    "keywords": ["laravel", "shoppingcart"],
    "license": "MIT",
    "authors": [
        {
            "name": "Rob Gloudemans",
            "email": "info@robgloudemans.nl"
        },
        {
            "name": "Hafiz Shehbaz Ali",
            "email": "shehbaz.bsee1395@gmail.com"
        }

    ],
    "require": {
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*",
        "illuminate/session": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*",
        "illuminate/events": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*",
        "illuminate/auth": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0|~6.0|~7.0",
        "mockery/mockery": "~0.9.0",
        "orchestra/testbench": "~3.1"
    },
    "autoload": {
        "psr-4": {
            "Redsignal\\Shoppingcart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Redsignal\\Tests\\Shoppingcart\\": "tests/"
        }
    },
    "suggest": {
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Redsignal\\Shoppingcart\\ShoppingcartServiceProvider"
            ],
            "aliases": {
                "Cartapi": "Redsignal\\Shoppingcart\\Facades\\Cart"
            }
        }
    }
}
